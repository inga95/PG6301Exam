    "parcel": "^2.5.0",
    "prettier": "^2.6.2"
  },
  "dependencies": {
    "@parcel/transformer-react-refresh-wrap": "^2.5.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0"
  },
  "babel": {
    "presets": [
